{
  "name": "tomasnorre/golf_courses",
  "type": "typo3-cms-extension",
  "description": "Displays which golf courses played",
  "license": ["GPL-2.0+"],
  "version": "1.0.0",
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "git",
      "url": "https://git.typo3.org/TYPO3CMS/Extensions/static_info_tables.git"
    }
  ],
  "require": {
    "typo3/cms": ">=8.7.0,<8.9.99",
    "sjbr/static-info-tables": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.6.0",
    "typo3/testing-framework": "~1.0.0"
  },
  "authors": [
    {
      "name": "Tomas Norre Mikkelsen",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "TNM\\GolfCourses\\": "Classes",
      "TNM\\GolfCourses\\Tests\\": "Tests"
    }
  },
  "autoload-dev": {
    "TYPO3\\TestingFramework\\Core\\": ".Build/vendor/typo3/testing-framework/Classes/Core"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/golf_course ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/golf_courses",
      "export TYPO3_PATH_WEB=$PWD/.Build/Web"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  },
  "replace": {
    "golf_courses": "self.version"
  }
}
